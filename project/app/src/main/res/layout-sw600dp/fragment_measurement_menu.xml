<!--                        android:layout_height="wrap_content"-->
<!--                        android:fontFamily="@font/font_family"-->
<!--                        android:letterSpacing="-0.01"-->
<!--                        android:text="스텝 검사"-->
<!--                        android:textColor="#000"-->
<!--                        android:textFontWeight="700"-->
<!--                        android:textSize="34sp" />-->

<!--                </LinearLayout>-->

<!--                <ImageView-->
<!--                    android:layout_width="wrap_content"-->
<!--                    android:layout_height="wrap_content"-->
